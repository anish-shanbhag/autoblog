    {
      "path": "dist/recipes.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "config": "workspace:*",
    "dts-cli": "^1.5.2",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "jest-watch-typeahead": "^2.0.0",
    "size-limit": "^7.0.8",
    "ts-jest": "^28.0.7",
    "tsconfig": "workspace:*",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "ora": "^6.1.2",
    "update-notifier": "^6.0.2"
