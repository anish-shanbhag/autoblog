{
  "name": "scaffolding",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "web": "cd apps/web && yarn dev",
    "lint": "turbo run lint",
    "remote-cache": "turbo link",
    "clean": "find . -type d -name node_modules -prune -exec rm -rf \\{\\} ';'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "turbo": "^1.3.1"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.2.1"
}
