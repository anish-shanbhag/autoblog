{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["$NODE_VERSION", "$RUNNER_OS"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
