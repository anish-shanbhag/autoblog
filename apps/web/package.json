{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "docker compose up --build --no-log-prefix",
    "watch": "chokidar ../../yarn.lock -c yarn --polling & blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "generate": "blitz prisma generate",
    "lint": "prettier --write --plugin=prettier-plugin-prisma --loglevel warn **/*.prisma && eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "dependencies": {
    "@blitzjs/auth": "alpha",
    "@blitzjs/next": "alpha",
    "@blitzjs/rpc": "alpha",
    "@hookform/resolvers": "2.9.5",
    "@prisma/client": "4.0.0",
    "blitz": "2.0.0-alpha.53",
    "next": "12.2.0",
    "prisma": "4.0.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "7.33.1",
    "ui": "workspace:*",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.8",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.16",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.0.0",
    "chokidar-cli": "^3.0.0",
    "config": "workspace:*",
    "eslint": "7.32.0",
    "jest": "27.5.1",
    "prettier": "^2.5.1",
    "prettier-plugin-prisma": "3.8.0",
    "preview-email": "3.0.5",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.3"
  }
